{
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "semi": [1, "never"],
      "no-console": 0
    },
    globals: {
      require: true,
      console: true,
      describe: true,
      it: true,
      beforeEach: true,
      expect: true
    }
}
